  "scripts": {
    "reset": "npx nx reset",
    "build": "npx nx run-many --target=build --all --configuration=development",
    "build:prod": "npx nx run-many --target=build --all --configuration=production",
    "test": "npx nx run-many --target=test --all",
    "test:ci": "npx nx run-many --target=test --all --configuration=ci",
    "lint": "npx nx run-many --target=lint --all",
    "patch": "npm version patch --no-git-tag-version && npx nx run-many --target=patch --all",
    "minor": "npm version minor --no-git-tag-version && npx nx run-many --target=minor --all",
    "major": "npm version major --no-git-tag-version && npx nx run-many --target=major --all",
    "affected:build": "npx nx affected --target=build --configuration=development",
    "affected:build:prod": "npx nx affected --target=build --configuration=production",
    "affected:test": "npx nx affected --target=test",
    "affected:lint": "npx nx affected --target=lint",
    "affected:publish": "npx nx affected --target=publish",
    "affected:patch": "npm version patch --no-git-tag-version && npx nx affected --target=patch",
    "affected:minor": "npm version minor --no-git-tag-version && npx nx affected --target=minor",
    "affected:major": "npm version major --no-git-tag-version && npx nx affected --target=major",
    "e2e": "npx nx e2e shell-e2e",
    "e2e:headless": "npx nx e2e shell-e2e --headless",
    "graph": "npx nx graph",
    "verdaccio": "npx verdaccio -c verdaccio.yaml",
    "publish:local": "npx nx run-many --target=publish-local --all",
    "publish": "npx nx run-many --target=publish --all",
    "unpublish:local": "npx nx run-many --target=unpublish-local --all",
    "watch": "node -r ts-node/register ./tools/watch.ts"
